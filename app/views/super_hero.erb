  <label>Hero Power:</label>
  <input type="text" id="member2_power" name="team[superheroes][][power]">
  <br>
  <label>Hero Biography:</label>
  <input type="text" id="member2_bio" name="team[superheroes][][bio]">
   <h1>Hero 3</h1>
